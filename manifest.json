{
  "manifest_version": 2,

  "name": "Search Like A Boss",
  "short_name": "SLAB",
  "description": "This extension adds tab indexes to the google search results page",
  "version": "0.1",

  "permissions": [
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": [
          "http://*.google.com/*",
          "https://*.google.com/*",
          "https://news.ycombinator.com/"
          ],
      "css": ["style.css"],
      "js": ["slab.js"],
      "run_at": "document_end"
    }
  ],

  "manifest_version": 2
}